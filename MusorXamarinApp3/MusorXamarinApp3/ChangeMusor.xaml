<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MusorXamarinApp3.ChangeMusor">
    <ContentPage.Content>
        <StackLayout>
			<StackLayout x:Name="stk1">
				<Button Text="Выбор партии" x:Name="Vibor" Clicked="Vibor_Clicked"/>
			</StackLayout>
			<Entry IsPassword="False" x:Name="NomerPartii" Placeholder="Номер паритии"/>
			<StackLayout x:Name="stk2">
				<Entry IsPassword="False" x:Name="Ves" Placeholder="Суммарный вес"/>
				<Entry IsPassword="False" x:Name="KontNomer" Placeholder="Номер контейнера"/>
				<Entry IsPassword="False" x:Name="MusorTipe" Placeholder="Тип отходов"/>
				<Button Text="Ввод" x:Name="enter" Clicked="enter_Clicked"/>
			</StackLayout>
		</StackLayout>
    </ContentPage.Content>
</ContentPage>