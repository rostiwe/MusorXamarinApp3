<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MusorXamarinApp3.KodInfoEnter">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Введите значения" />
			<Entry IsPassword="false" Placeholder="вес" x:Name="ves"/>
			<Entry IsPassword="false" Placeholder="Номер партии" x:Name="Number"/>
			<Button Text="Сканировать" x:Name="ScanButton" Clicked="ScanButton_Clicked"/>
			<Button Text="Внести сведения" x:Name="ButtonEnter" Clicked="ButtonEnter_Clicked"/>
		</StackLayout>
    </ContentPage.Content>
</ContentPage>