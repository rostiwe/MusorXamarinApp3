<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MusorXamarinApp3.LoginPage1">
    <ContentPage.Content>
        <StackLayout>
			<StackLayout>
				<Label Text="Введите логин и пароль" />
				<Entry IsPassword="False" Placeholder="Логин" x:Name="loginString"/>
				<Entry IsPassword="True" Placeholder="Пароль" x:Name="passwordString"/>
				<Button Text="Вход" x:Name="Enterbutton" Clicked="Enterbutton_Clicked"></Button>
			</StackLayout>
		</StackLayout>
    </ContentPage.Content>
</ContentPage>